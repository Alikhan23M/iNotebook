{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js",
       "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "headers":{
        "Access-Control-Allow-Origin":"*"
    }
    }
  ],
   "functions": {
    "api/*.js": {
      "maxDuration": 10 // Increase the duration as needed
    }
  }
}
